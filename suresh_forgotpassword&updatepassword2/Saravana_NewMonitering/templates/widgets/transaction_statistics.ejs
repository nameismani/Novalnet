        <ul class=" nav nav-pills w-4/5 bg-slate-100 dark:bg-black/20 rounded-md mx-auto " role="tablist">
            <li id="active-users-tab" class="nav-item flex-1" role="presentation">
                <button class="nav-link w-full py-1.5 px-2 active" data-tw-toggle="pill" data-tw-target="#active-users" type="button" role="tab" aria-controls="active-users" aria-selected="true"> Live </button>
            </li>
            <li id="inactive-users-tab" class="nav-item flex-1" role="presentation">
                <button class="nav-link w-full py-1.5 px-2" data-tw-toggle="pill" data-tw-target="#inactive-users" type="button" role="tab" aria-selected="false"> Test </button>
            </li>
        </ul>
        <div class="tab-content mt-6">
